<div class='container'>
<%= simple_form_for [user, @service] do |f| %>
  <%= f.input :title,  input_html: { class: 'title' } %>
  <%= f.input :description, input_html: { class: 'description' } %>
  <%= f.input :item, input_html: { class: 'item' } %>
  <%= f.input :price, input_html: {class: 'price' } %>
  <%= f.submit :create, class: "btn btn-success text-white" %>
<% end %>
</div>
